{
  "manifest_version": 2,
  "name": "AddPrefix for Bitbucket",
  "version": "1.0.2",
  "description": "Add branch name prefix for bitbucket",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://bitbucket.org/*branch*"],
      "js": ["jquery.min.js","script.js"],
      "run_at": "document_end"
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "storage"
  ]
}
